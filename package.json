{
  "name": "src",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/start-webpp",
    "build-dev": "rimraf backend/server.development.js && webpack --mode development --config webpack.config.js --watch",
    "start-mon": "nodemon ./backend/server.development.js",
    "build-prod": "rimraf backend/server.production.js && webpack --mode production --config webpack.config.js",
    "start-prod": "node ./backend/server.production.js",
    "socket-start": "node ./src/bin/start-socketapp",
    "socket-build-dev": "rimraf backend/socket.development.js && webpack --mode development --config webpack.socket.config.js --watch",
    "socket-start-mon": "nodemon ./backend/socket.development.js",
    "socket-build-prod": "rimraf backend/socket.production.js && webpack --mode production --config webpack.socket.config.js",
    "socket-start-prod": "node ./backend/socket.production.js"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "email-templates": "^7.0.4",
    "express": "~4.17.1",
    "express-jwt": "^5.3.1",
    "http-errors": "~1.7.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.5",
    "pug": "^2.0.4",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "socket.io": "^2.3.0",
    "sprintf-js": "^1.1.2",
    "ws-reconnect": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "app-module-path": "^2.2.0",
    "babel-loader": "^8.0.6",
    "nodemon": "^2.0.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "webpack-obfuscator": "^0.27.2"
  }
